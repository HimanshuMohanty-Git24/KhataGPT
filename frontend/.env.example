# API Configuration
REACT_APP_API_URL=http://localhost:8000
REACT_APP_API_TIMEOUT=30000

# Feature Flags
REACT_APP_ENABLE_AUTH=true
REACT_APP_ENABLE_ANALYTICS=false
REACT_APP_ENABLE_NOTIFICATIONS=true

# Environment settings
NODE_ENV=development
REACT_APP_ENV=development

# Authentication
REACT_APP_AUTH_DOMAIN=your-auth-domain.com
REACT_APP_AUTH_CLIENT_ID=your-client-id
REACT_APP_AUTH_REDIRECT_URI=http://localhost:3000/callback

# Analytics (if used)
REACT_APP_ANALYTICS_ID=UA-XXXXXXXXX-X

# Storage (for localStorage/sessionStorage keys)
REACT_APP_STORAGE_PREFIX=khathagpt_

# Optional integrations
REACT_APP_SENTRY_DSN=
REACT_APP_INTERCOM_APP_ID=

# Document processing limits
REACT_APP_MAX_UPLOAD_SIZE_MB=15
REACT_APP_MAX_UPLOADS_PER_REQUEST=5